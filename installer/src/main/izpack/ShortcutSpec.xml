<shortcuts>
    <programGroup defaultName="JBackup" location="applications/startMenu"/>
    <shortcut name="JBackup"
              target="$INSTALL_PATH/bin/jbackup.bat"
              commandLine=""
              workingDirectory="$INSTALL_PATH/bin"
              description="JBackup"
              initialState="normal"
              programGroup="yes"
              desktop="yes"
              applications="no"
              startMenu="no"
              startup="no">
        <createForPack name="Base"/>
    </shortcut>
    <shortcut name="Uninstall"
              target="$INSTALL_PATH/Uninstaller/uninstaller.jar"
              workingDirectory="$INSTALL_PATH/Uninstaller"
              description=""
              initialState="normal"
              programGroup="yes"
              desktop="no"
              applications="no"
              startMenu="no"
              startup="no">
        <createForPack name="Base"/>
    </shortcut>
</shortcuts>

